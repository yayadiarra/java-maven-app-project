pipeline {
agent any
  tools {
    maven "maven-diarra"
  }
  stages {
    stage ('Maven Clean'){
      steps{
        sh 'mvn clean'
      }
    }
  }
       withDockerRegistry([ credentialsId: "Docker_creds", url: "https://index.docker.io/v1/" ]){
               sh 'docker build -t devopstrainingschool/java-maven-jenkins . -f Dockerfile'
               sh 'docker push devopstrainingschool/java-maven-jenkins'
             }
           
    }
